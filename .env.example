# .env.example - Template for environment variables
# Copy this file to .env and fill in your actual values
# NEVER commit the .env file to version control

# Admin Credentials
ADMIN_EMAIL=your_admin_email@domain.com
ADMIN_PASSWORD=your_secure_password_here

# Firebase Configuration
FIREBASE_PROJECT_ID=your-firebase-project-id
FIREBASE_DATABASE_URL=https://your-project.firebaseio.com
FIREBASE_API_KEY_WEB=your_web_api_key_here
FIREBASE_API_KEY_ANDROID=your_android_api_key_here
FIREBASE_API_KEY_IOS=your_ios_api_key_here
FIREBASE_API_KEY_WINDOWS=your_windows_api_key_here
FIREBASE_AUTH_DOMAIN=your-project.firebaseapp.com
FIREBASE_STORAGE_BUCKET=your-project.appspot.com
FIREBASE_MESSAGING_SENDER_ID=your_sender_id
FIREBASE_APP_ID_WEB=your_web_app_id
FIREBASE_APP_ID_ANDROID=your_android_app_id
FIREBASE_APP_ID_IOS=your_ios_app_id
FIREBASE_APP_ID_WINDOWS=your_windows_app_id
FIREBASE_MEASUREMENT_ID=G-XXXXXXXXXXX

# Email Service Configuration
EMAIL_SENDER_ADDRESS=your_email@gmail.com
EMAIL_APP_PASSWORD=your_app_specific_password
EMAIL_SENDER_NAME=Your App Name
EMAIL_APP_URL=https://your-app-url.com

# SMTP Configuration
SMTP_HOST=smtp.gmail.com
SMTP_PORT=587
SMTP_SECURE=false

# Demo Account
DEMO_PASSWORD=demo_password_here